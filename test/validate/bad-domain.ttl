@prefix eg: <http://example.org/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

eg:Thing
	a rdfs:Class .

eg:NonThing
	a rdfs:Class .

eg:value
	a rdf:Property ;
	rdfs:label "value" ;
	rdfs:domain eg:Thing .

eg:nonthing
	a eg:NonThing ;
	eg:value 42 .

